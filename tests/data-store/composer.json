{
    "name": "opis/data-store",
    "description": "Opis Data Store is a library for storing and fetching structured collections of data.",
    "keywords": ["opis", "data-store", "data", "config"],
    "homepage": "https://opis.io/data-store",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Marius Sarca",
            "email": "marius.sarca@gmail.com"
        },
        {
            "name": "Sorin Sarca",
            "email": "sarca_sorin@hotmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Opis\\DataStore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opis\\DataStore\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
